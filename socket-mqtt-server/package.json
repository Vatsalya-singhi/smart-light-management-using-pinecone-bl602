  "dependencies": {
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "mqtt": "^5.3.4",
    "nodemon": "^3.0.2",
    "socket.io": "^4.7.2",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21"
  }
}